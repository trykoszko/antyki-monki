<h1>
  {{ __('OLX', wpData.textdomain) }}
</h1>

<div class="wrap wp-olx-wrap">
  <div id="dashboard-widgets-wrap">
    <div id="dashboard-widgets" class="metabox-holder">
      <div id="postbox-container-1" class="postbox-container">
        <div id="normal-sortables" class="meta-box-sortables ui-sortable">
          {{
            include(
              'partials/adminBoxes/endingAdverts.twig',
              {
                endingAdverts: endingAdverts
              }
            )
          }}
          {{
            include(
              'partials/adminBoxes/packets.twig',
              {
                packets: packets
              }
            )
          }}
        </div>
      </div>

      <div id="postbox-container-2" class="postbox-container">
        <div id="normal-sortables" class="meta-box-sortables ui-sortable">
          {{
            include(
              'partials/adminBoxes/messages.twig',
              {
                messages: messages
              }
            )
          }}
          {{
            include(
              'partials/adminBoxes/userInfo.twig',
              {
                userInfo: userInfo
              }
            )
          }}
        </div>
      </div>
    </div>
  </div>
</div>
